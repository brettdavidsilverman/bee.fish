      <h2>Or</h2>
      
      <details>
         <summary>
A class to match a single item from an
array of input items.
         </summary>
         
         <pre>
            <script class="export">
class Or extends Match {
   #item;
   constructor(...inputs) {
      super(...inputs);
   }
   
   match(character) {
      var matched = false;
 
      for (var i = 0;
           i < this.inputs.length;
           i++)
      {
         var item = this.inputs[i];
         if (item.success === undefined) {
         
            if (item.match(character))
               matched = true;
            
            if (item.success) {
               this.#item = item;
               this.index = i;
               this.success = true;
               return matched;
            }
            
         }
      }
      
      if (!matched)
         this.success = false;
         
      return matched;
   }
   
   readEnd() {
      
      for (var i = 0;
           i < this.inputs.length;
           i++)
      {
         var item = this.inputs[i];
         if (item.success != false) {
            item.readEnd();
            if (item.success) {
               this.#item = item;
               this.index = i;
               this.success = true;
               break;
            }
         }
      }
      
      super.readEnd();

   }
   
   get item() {
      return this.#item;
   }
   
   get value() {
      return this.item.value;
   }
   
}
            </script>
         </pre>
  
