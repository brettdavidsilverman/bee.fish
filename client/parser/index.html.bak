         ),
         new Character("b"),
         new Optional(
            new Character("-")
         ),
         new Character("c"),
         new Optional(
            new Character(".")
         )
      );
   }
}

var test;
test = new TestOptional();
test.read("abc");
test.write(document);

test = new TestOptional();
test.read("a-b-c");
test.write(document);
   
