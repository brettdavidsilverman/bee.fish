   constructor() {
      super("A", "Z");
   }
}

var range = new RangeAZ();
range.read("B");
range.write(document);

range = new RangeAZ();
range.read("b");
range.write(document);

               </script>
            </pre>
         </details>
      </details>
      
      <h2>Word</h2>
     
      <details>
      
         <summary>
A class to match a string of 
characters.
         </summary>
         
         <pre>
            <script class="export">
class Word extends Match {
   #index = 0;
   #word;
   constructor(word) {
      super();
      this.#word = word;
   }
   
   match(character) {
      var matched =
         this.#word[this.#index] ===
         character;
         
      if (matched)
      {
         super.match(character);
         ++this.#index;
         if (this.#index === this.#word.length) {
            this.success = true;
         }
      }
      else
         this.success = false;
         
      return matched;
   }
   
   
}

            </script>
         </pre>
